{
    "name": "rinvex/category",
    "description": "Rinvex Category is a polymorphic Laravel package, for category management. You can categorize any eloquent model with ease, and utilize the power of nested sets.",
    "keywords": [
        "model",
        "laravel",
        "eloquent",
        "category",
        "categorizable",
        "polymorphic",
        "nested-set",
        "taxonomy",
        "rinvex"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/category/issues",
        "source": "https://github.com/rinvex/category",
        "docs": "https://github.com/rinvex/category/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/category/contributors"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "kalnoy/nestedset": "^4.1.0",
        "illuminate/database": "^5.1.0",
        "spatie/laravel-sluggable": "^1.3.0",
        "spatie/eloquent-sortable": "^3.1.0",
        "spatie/laravel-translatable": "^1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4.0",
        "illuminate/container": "^5.1.0"
    },
    "autoload": {
        "psr-4": {
            "Rinvex\\Category\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rinvex\\Category\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
